    ["@babel/preset-env"],
    ["@babel/preset-typescript", { "onlyRemoveTypeImports": true }],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-transform-runtime",
    "babel-plugin-transform-imports"
  ]
}
